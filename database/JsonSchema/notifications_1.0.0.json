{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "oneOf": [
        {
            "type": "object",
            "description": "Payload for bookmarksAddedToFolder notification.",
            "properties": {
                "N-type": {
                    "type": "string",
                    "enum": [
                        "bookmarksAddedToFolder"
                    ]
                },
                "version": {
                    "type": "string",
                    "enum": [
                        "1.0.0"
                    ]
                },
                "bookmarks_added_to_folder": {
                    "type": "array",
                    "description": "The bookmark ids that were added to the folder. The maxItems in this property should collaboration correspond with MAX_POST_FOLDER_BOOKMARKS setting",
                    "minItems": 1,
                    "maxItems": 50,
                    "uniqueItems": true,
                    "items": {
                        "type": "integer",
                        "minimum": 1
                    }
                },
                "added_to_folder": {
                    "type": "integer",
                    "minimum": 1,
                    "description": "The folderID the new bookmarks where added to"
                },
                "added_by": {
                    "type": "integer",
                    "minimum": 1,
                    "description": "The userID of the collaborator that added the bookmarks"
                }
            },
            "additionalProperties": false,
            "required": [
                "bookmarks_added_to_folder",
                "added_to_folder",
                "added_by",
                "N-type",
                "version"
            ]
        },
        {
            "type": "object",
            "description": "Payload for bookmarks removed from folder notification",
            "properties": {
                "N-type": {
                    "type": "string",
                    "enum": [
                        "bookmarksRemovedFromFolder"
                    ]
                },
                "version": {
                    "type": "string",
                    "enum": [
                        "1.0.0"
                    ]
                },
                "bookmarks_removed": {
                    "type": "array",
                    "description": "The bookmark ids that were removed from the folder. The maxItems in this property should collaboration correspond with MAX_DELETE_FOLDER_BOOKMARKS setting",
                    "minItems": 1,
                    "maxItems": 50,
                    "uniqueItems": true,
                    "items": {
                        "type": "integer",
                        "minimum": 1
                    }
                },
                "removed_from_folder": {
                    "type": "integer",
                    "minimum": 1,
                    "description": "The folderID of the bookmarks where removed from"
                },
                "removed_by": {
                    "type": "integer",
                    "minimum": 1,
                    "description": "The userID of the collaborator that removed the bookmarks"
                }
            },
            "additionalProperties": false,
            "required": [
                "bookmarks_removed",
                "removed_from_folder",
                "removed_by",
                "N-type",
                "version"
            ]
        },
        {
            "type": "object",
            "description": "Payload for collaboratorExit from folder notification",
            "properties": {
                "N-type": {
                    "type": "string",
                    "enum": [
                        "collaboratorExitedFolder"
                    ]
                },
                "version": {
                    "type": "string",
                    "enum": [
                        "1.0.0"
                    ]
                },
                "exited_from_folder": {
                    "type": "integer",
                    "minimum": 1,
                    "description": "The folderID the collaborator exited from"
                },
                "exited_by": {
                    "type": "integer",
                    "minimum": 1,
                    "description": "The collaboratorID that exited the folder collaboration"
                }
            },
            "additionalProperties": false,
            "required": [
                "exited_from_folder",
                "exited_by",
                "N-type",
                "version"
            ]
        },
        {
            "type": "object",
            "description": "Payload for folderUpdated notification",
            "properties": {
                "N-type": {
                    "type": "string",
                    "enum": [
                        "FolderUpdated"
                    ]
                },
                "version": {
                    "type": "string",
                    "enum": [
                        "1.0.0"
                    ]
                },
                "updated_by": {
                    "type": "integer",
                    "minimum": 1,
                    "description": "The collaboratorID of the user that updated the folder"
                },
                "folder_updated": {
                    "type": "integer",
                    "minimum": 1,
                    "description": "The folderID that was updated"
                },
                "changes": {
                    "type": "object",
                    "description": "The changes that were made for the folder update",
                    "properties": {
                        "name": {
                            "type": "object",
                            "description": "The changes that were applied to the folder name if any",
                            "properties": {
                                "from": {
                                    "type": "string",
                                    "description": "The initial name of the folder before it was changes"
                                },
                                "to": {
                                    "type": "string",
                                    "description": "The new value the folder name was changed to"
                                }
                            },
                            "additionalProperties": false,
                            "required": [
                                "from",
                                "to"
                            ]
                        },
                        "description": {
                            "type": "object",
                            "description": "The changes that were applied to the folder description if any",
                            "properties": {
                                "from": {
                                    "type": "string",
                                    "description": "The initial name of the folder description before it was changes"
                                },
                                "to": {
                                    "type": "string",
                                    "description": "The new value the folder description was changed to"
                                }
                            },
                            "additionalProperties": false,
                            "required": [
                                "from",
                                "to"
                            ]
                        },
                        "tags": {
                            "type": "object",
                            "description": "The changes that were applied to the folder tags if any",
                            "properties": {
                                "from": {
                                    "type": "string",
                                    "description": "The initial tags of the folder before it was changes"
                                },
                                "to": {
                                    "type": "string",
                                    "description": "The new values the folder tags were changed to"
                                }
                            },
                            "additionalProperties": false,
                            "required": [
                                "from",
                                "to"
                            ]
                        }
                    },
                    "additionalProperties": false
                }
            },
            "additionalProperties": false,
            "required": [
                "updated_by",
                "folder_updated",
                "changes",
                "version",
                "N-type"
            ]
        },
        {
            "type": "object",
            "description": "Payload for new folder collaborator notification",
            "properties": {
                "N-type": {
                    "type": "string",
                    "enum": [
                        "collaboratorAddedToFolder"
                    ]
                },
                "version": {
                    "type": "string",
                    "enum": [
                        "1.0.0"
                    ]
                },
                "added_by_collaborator": {
                    "type": "integer",
                    "minimum": 1,
                    "description": "The userID of the collaborator that added the new collaborator"
                },
                "added_to_folder": {
                    "type": "integer",
                    "minimum": 1,
                    "description": "The folderID that the new collaborator was added to"
                },
                "new_collaborator_id": {
                    "type": "integer",
                    "minimum": 1,
                    "description": "The userID of the new collaborator"
                }
            },
            "additionalProperties": false,
            "required": [
                "added_by_collaborator",
                "added_to_folder",
                "new_collaborator_id",
                "N-type",
                "version"
            ]
        }
    ]
}